
{
    "regions": ["iad1"],
    "builds": [
      { "src": "back/server.js", "use": "@now/node" },
      {
        "src": "front/html/index.html"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "headers": { "cache-control": "s-maxage=0" },
        "dest": "back/server.js"
      },
      {
        "src": "/front/html/(.*)",
        "headers": { "cache-control": "s-maxage=31536000, immutable" },
        "dest": "front/html/$1"
      },
      { "src": "/(.*)", "dest": "front/html/index.html" }
    ]
  }